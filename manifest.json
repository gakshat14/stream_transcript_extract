{
    "name": "Stream Transcript Extractor",
    "description": "Extension to allow users to extract transcript from Micsoroft Stream website.",
    "version": "1.0",
    "manifest_version": 3,
    "devtools_page": "devtools.html",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "transcript_logo.png"
    },
    "icons": {
        "64": "transcript_logo.png"
    }
}